{
    "name": "ringlives/easypayment",
    "description": "Ringlives easypayment provides a featherweight payment gateway.",
    "keywords": ["laravel", "easypayment", "bkash", "Gateway", "Payment Gateway"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ringlives/easypayment/issues",
        "source": "https://github.com/ringlives/easypayment"
    },
    "authors": [
        {
            "name": "Md. Sahidur Rahman",
            "email": "shohidurr49@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "moneyphp/money": "^3.2|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ringlives\\Easypayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ringlives\\Easypayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ringlives\\Easypayment\\EasypaymentServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
